<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="view_picking_tree_inherit_pos_receipt" model="ir.ui.view">
      <field name="name">stock.picking.tree.inherit.pos.receipt</field>
      <field name="model">stock.picking</field>
      <field name="inherit_id" ref="stock.vpicktree"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='state']" position="before">
          <field name="pos_order_id" string="Orden" widget="many2one_clickable" optional="show"/>
        </xpath>
      </field>
    </record>
  </data>
</odoo>